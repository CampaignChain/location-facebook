{
    "name": "campaignchain/location-facebook",
    "description": "Holds information about Facebook locations such as user and page stream, as well as status messages.",
    "keywords": ["facebook","campaignchain"],
    "type": "campaignchain-location",
    "homepage": "http://www.campaignchain.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "CampaignChain, Inc.",
            "email": "info@campaignchain.com",
            "homepage": "http://www.campaignchain.com"
        }
    ],
    "support": {
        "issues": "https://github.com/CampaignChain/campaignchain-ce/issues",
        "docs": "http://doc.campaignchain.com"
    },
    "require": {
        "campaignchain/channel-facebook": "dev-master"
    },
    "autoload": {
        "psr-4": { "CampaignChain\\Location\\FacebookBundle\\": "" }
    },
    "extra": {
        "campaignchain": {
            "kernel": {
                "classes": [
                    "CampaignChain\\Location\\FacebookBundle\\CampaignChainLocationFacebookBundle"
                ]
            }
        }
    }
}
